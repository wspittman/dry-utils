{
  "name": "dry-utils",
  "version": "0.1.0",
  "description": "A tree-shakeable TypeScript utility library",
  "type": "module",
  "exports": {
    "./async": {
      "types": "./dist/async/index.d.ts",
      "import": "./dist/async/index.js",
      "default": "./dist/async/index.js"
    },
    "./htmldown": {
      "types": "./dist/htmldown.d.ts",
      "import": "./dist/htmldown.js",
      "default": "./dist/htmldown.js"
    },
    "./logger": {
      "types": "./dist/logger.d.ts",
      "import": "./dist/logger.js",
      "default": "./dist/logger.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "node --import tsx/esm --test test/**/*.test.ts",
    "pretest": "tsc --noEmit"
  },
  "license": "MIT",
  "dependencies": {
    "@azure/cosmos": "^4.2.0",
    "html-entities": "^2.5.2",
    "sanitize-html": "^2.14.0",
    "showdown": "^2.1.0",
    "turndown": "^7.2.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/sanitize-html": "^2.13.0",
    "@types/showdown": "^2.0.6",
    "@types/turndown": "^5.0.5",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
}
